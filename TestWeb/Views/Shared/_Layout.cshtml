<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Product Angular Page</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
   
    <div class="container body-content">
        @RenderBody()        
     
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/angularroute")
    @RenderSection("scripts", required: false)
    <script src="~/app/productModule.js" type="text/javascript"></script>
    <script src="~/app/services/productService.js" type="text/javascript"></script>
    <script src="~/app/controller/productController.js" type="text/javascript"></script>
</body>
</html>
